{
    "name": "PS AIE Kernel (C)", 
    "description": [
        "This PS AIE kernel tests PS kernel controlling AIE."
    ],
    "flow": "vitis",
    "key_concepts": [
        "PS Kernel", 
        "AIE"
    ], 
    "os": [
        "Linux"
    ], 
    "flow": "versal",
    "platform_type": "pcie",	
    "platform_allowlist": [ 
        "vck5000"
    ],
    "platform_blocklist": [
        "_u25_",
        "u55",
        "u50",
        "u30",
        "u200",
        "zc",
        "vck190",
        "u250",
        "aws-vu9p-f1",
        "samsung",
        "_u2_",
        "x3522pv",
        "nodma"
    ],
    "gui": false,     
    "host": {
        "compiler": {
            "sources": [
                "./src/host.cpp"
            ], 
            "includepaths": [
            ]
        },
        "linker" : {
            "libraries" : [
                "boost_filesystem",
                "boost_system"
            ]
        }, 
        "host_exe": "ps_aie_gmio_test.exe"
    },
    "aiecompiler": {
        "compiler": {
            "includepaths": [
              "PROJECT/src",
              "PROJECT/src/kernels"
            ]
        }
    }, 
    "aiecontainers": [
        {
            "accelerators": [
                {
                    "location": "PROJECT/src/graph.cpp", 
                    "workdir": "./Work", 
                    "name": "libadf.a"
                }
            ]
         }
    ],
    "match_makefile": "false",
    "config_make": "config.mk",        
    "launch": [
        {
            "cmd_args": "-p $(GEN_DIR)", 
            "name": "generic launch for all flows"
        }
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "targets": [
        "hw"
    ], 
    "testinfo": {
        "profile": "no",
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_hw"
        ],
        "category": "canary"
    }
}
