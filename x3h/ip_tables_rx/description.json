{
    "name": "Xilinx IPTables RX Filter",
    "description": [
        "Xilinx IPTables RX Filter offload engine."
    ],
    "flow": "vitis",
    "default_device": "xilinx_x3522p_gen4x8_hn10g_1_202220_1",
    "platform_type": "pcie",
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ],
    "platform_blocklist": [
        "_u25_",
        "u200",
	"u250",
	"u280",
	"u50_gen3x16",
	"u55c_gen3x16",
        "u30",
        "u50lv",
        "u50_gen3x4",
        "zc",
        "vck", 
        "aws-vu9p-f1",
        "samsung",
        "_u2_"
    ],
    "match_makefile" : "false",
    "match_ini" : "false",
    "host": {
        "host_exe": "host.exe",
        "compiler": {
            "sources": [
                "./src/host.cpp",
                "LIB_DIR/common/libs/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L2/tests/src/"
            ]
        }
    },
    "containers": [
        {
            "name": "ip_tables_rx",
            "accelerators": [
                {
                    "name": "controlUnit",
                    "location": "src/kernel/iptfilterRxsc.cpp"
                },
                {
                    "name": "rxNicInterface",
                    "location": "src/kernel/iptfilterRxsc.cpp"
                },
                {
                    "name": "ethernetParser",
                    "location": "src/kernel/iptfilterRxsc.cpp"
                },
                {
                    "name": "ipv4Parser",
                    "location": "src/kernel/iptfilterRxsc.cpp"
                },
                {
                    "name": "ruleManager",
                    "location": "src/kernel/iptfilterRxsc.cpp"
                },
                {
                    "name": "packetFilter",
                    "location": "src/kernel/iptfilterRxsc.cpp"
                },
                {
                    "name": "rxMacInterface",
                    "location": "src/kernel/iptfilterRxsc.cpp"
                }
            ]
        }
    ],
    "launch": [
        {
            "name": "generic launch for all flows"
        }
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 300,
                    "vitis_sw_emu": 25,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
