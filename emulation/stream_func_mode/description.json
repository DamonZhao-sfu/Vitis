{
    "name": "Functional Hardware Emulation (Stream Free Running HLS C/C++ Kernel)", 
    "description": [
        "This example demonstrates how a user can run functional model of HLS kernel in HW Emulation."
    ],
    "flow": "vitis",
    "match_makefile": "false",
    "keywords": [
        "emulationMode=func" 
    ], 
    "key_concepts": [
        "Emulation Functional mode" 
    ], 
    "platform_blacklist": [ 
        "nodma"
    ],
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "host": {
        "host_exe": "stream_func_mode",
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2/xcl2.cpp",
                "./src/host.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "mem_read1", 
                    "location": "src/mem_read1.cpp"
                },
                {
                    "name": "increment1", 
                    "location": "src/increment1.cpp"
                },
                {
                    "name": "mem_write1", 
                    "location": "src/mem_write1.cpp"
                },
                {
                    "name": "mem_read2", 
                    "location": "src/mem_read2.cpp"
                },
                {
                    "name": "increment2", 
                    "location": "src/increment2.cpp"
                },
                {
                    "name": "mem_write2", 
                    "location": "src/mem_write2.cpp"
                }
            ], 
            "name": "krnl_incr",
            "ldclflags": "--config PROJECT/krnl_incr.cfg"
        }
    ],
    "launch": [
        {
            "cmd_args": "BUILD/krnl_incr.xclbin", 
            "name": "generic launch for all flows"
        }
    ],  
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
