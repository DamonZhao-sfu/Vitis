{
    "name": "P2P Simple Example", 
    "description": [
        "This is simple example of vector increment to describe P2P between FPGA and NVMe SSD."
    ],
    "keywords": [
        "XCL_MEM_EXT_P2P_BUFFER",
        "pread",
        "pwrite", 
        "O_DIRECT",
        "O_RDWR"
    ],
    "key_concepts": [
        "P2P", 
        "NVMe SSD",
        "SmartSSD"
    ],
    "platform_type": "pcie",
    "os": [
        "Linux"
    ],
    "ndevice": [
        "zc",
        "u25",
        "u200",
        "u250",
        "u280",
        "u50",
        "vck",
        "aws"
    ],
    "platform_blacklist": [
        "zc",
        "u25",
        "u200",
        "u250",
        "u280",
        "u50",
        "vck",
        "aws"
    ],
     "runtime": [
        "OpenCL"
    ], 
    "launch": [
        {
            "cmd_args": "-x BUILD/adder.xclbin -i /dev/nvme0n1", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host", 
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2",
                "REPO_DIR/common/includes/cmdparser",
                "REPO_DIR/common/includes/logger"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2",
                "REPO_DIR/common/includes/cmdparser",
                "REPO_DIR/common/includes/logger"
            ]
        }
    },  
    "containers": [
        {
            "accelerators": [
                {
                    "name": "adder", 
                    "location": "src/adder.cpp"                    
                }
            ], 
            "name": "adder",
            "ldclflags": "--config PROJECT/adder.ini"
        }
    ],
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ] 
}
