                            "arguments": [
                                {
                                    "name": "in", 
                                    "memory": "DDR[0]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR1", 
                            "arguments": [
                                {
                                    "name": "in", 
                                    "memory": "DDR[1]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR1", 
                            "arguments": [
                                {
                                    "name": "in", 
