{
    "name": "Xilinx GZIP Application for u200 and u250 Platforms", 
    "description": [
        "GZip application for DDR resides in `demos/gzip_app` directory."
    ], 
    "flow": "vitis", 
    "platform_blacklist": [
        "vck", 
        "u50", 
        "u280"
    ], 
     "runtime": [
        "OpenCL"
    ], 
    "platform_whitelist": [
        "aws"
    ], 
    "launch": [
        {
            "cmd_args": "-sx BUILD/compress_decompress.xclbin -v PROJECT/sample.txt", 
            "name": "generic launch for all flows"
        }
    ], 
    "match_ini": "false", 
    "config_make": "./config.mk", 
    "host": {
        "host_exe": "xil_gzip", 
        "compiler": {
            "sources": [
                "PROJECT/src/host.cpp", 
                "PROJECT/src/zlib.cpp", 
                "PROJECT/src/cmdlineparser.cpp", 
                "PROJECT/src/logger.cpp", 
                "PROJECT/src/xcl2.cpp", 
                "PROJECT/src/crc32.c", 
                "PROJECT/src/adler32.c" 
            ], 
            "includepaths": [
                "PROJECT/include",
                "PROJECT/thirdParty"
            ],
            "options": [
                "-O3"
            ],
            "symbols": [
                "C_COMPUTE_UNIT=2", 
                "H_COMPUTE_UNIT=2", 
                "D_COMPUTE_UNIT=8", 
                "PARALLEL_BLOCK=8", 
                "GZIP_MODE", 
                "OVERLAP_HOST_DEVICE"
            ]
        }
    }, 
    "v++": {
        "build_datafiles": [
            "PROJECT/advanced.ini", 
            "PROJECT/hls_directives.tcl"
        ], 
        "compiler": {
            "includepaths": [
                "PROJECT/include"
            ],
            "symbols": [
                "PARALLEL_BLOCK=8", 
                "MULTIPLE_BYTES=8", 
                "GZIP_MODE"
            ], 
            "clflags": [
                "--config PROJECT/advanced.ini"
            ]
        }
    }, 
    "containers": [
        {
            "name": "compress_decompress", 
            "ldclflags": "--kernel_frequency 250 --config PROJECT/opts.ini", 
            "accelerators": [
                {
                    "name": "xilLz77Compress", 
                    "location": "./src/zlib_lz77_compress_mm.cpp", 
                    "num_compute_units": "2", 
                    "compute_units": [
                        {
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "in", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "out", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "compressd_size", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "in_block_size", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "dyn_ltree_freq", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "dyn_dtree_freq", 
                                    "memory": "DDR[0]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR2", 
                            "arguments": [
                                {
                                    "name": "in", 
                                    "memory": "DDR[3]"
                                }, 
                                {
                                    "name": "out", 
                                    "memory": "DDR[3]"
                                }, 
                                {
                                    "name": "compressd_size", 
                                    "memory": "DDR[3]"
                                }, 
                                {
                                    "name": "in_block_size", 
                                    "memory": "DDR[3]"
                                }, 
                                {
                                    "name": "dyn_ltree_freq", 
                                    "memory": "DDR[3]"
                                }, 
                                {
                                    "name": "dyn_dtree_freq", 
                                    "memory": "DDR[3]"
                                }
                            ]
                        }
                    ]
                }, 
                {
                    "name": "xilHuffmanKernel", 
                    "location": "./src/zlib_huffman_enc_mm.cpp", 
                    "num_compute_units": "2", 
                    "compute_units": [
                        {
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "in", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "out", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "in_block_size", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "compressd_size", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "dyn_ltree_freq", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "dyn_dtree_freq", 
                                    "memory": "DDR[0]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR2", 
                            "arguments": [
                                {
                                    "name": "in", 
                                    "memory": "DDR[3]"
                                }, 
                                {
                                    "name": "out", 
                                    "memory": "DDR[3]"
                                }, 
                                {
                                    "name": "in_block_size", 
                                    "memory": "DDR[3]"
                                }, 
                                {
                                    "name": "compressd_size", 
                                    "memory": "DDR[3]"
                                }, 
                                {
                                    "name": "dyn_ltree_freq", 
                                    "memory": "DDR[3]"
                                }, 
                                {
                                    "name": "dyn_dtree_freq", 
                                    "memory": "DDR[3]"
                                }
                            ]
                        }
                    ]
                }, 
                {
                    "name": "xilZlibDmReader", 
                    "location": "./src/zlib_dm_multibyte_rd.cpp", 
                    "num_compute_units": "8",
                    "compute_units": [
                        {
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "out", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "encoded_size", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "status_flag", 
                                    "memory": "DDR[0]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "out", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "encoded_size", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "status_flag", 
                                    "memory": "DDR[0]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR1", 
                            "arguments": [
                                {
                                    "name": "out", 
                                    "memory": "DDR[1]"
                                }, 
                                {
