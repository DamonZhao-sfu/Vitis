                        }
                    ]
                }, 
                {
                    "name": "xilDecompressFull", 
                    "location": "./src/zlib_parallelbyte_decompress_full.cpp", 
                    "num_compute_units": "8", 
                    "compute_units": [
                        {
                            "slr": "SLR0"
                        }, 
                        {
                            "slr": "SLR0"
                        }, 
                        {
                            "slr": "SLR1"
                        }, 
                        {
                            "slr": "SLR1"
                        }, 
                        {
