                                }, 
                                {
                                    "name": "encoded_size", 
                                    "memory": "DDR[1]"
                                }, 
                                {
                                    "name": "status_flag", 
                                    "memory": "DDR[1]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR1", 
                            "arguments": [
                                {
                                    "name": "out", 
                                    "memory": "DDR[2]"
                                }, 
                                {
                                    "name": "encoded_size", 
                                    "memory": "DDR[2]"
                                }, 
                                {
                                    "name": "status_flag", 
                                    "memory": "DDR[2]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR1", 
                            "arguments": [
                                {
                                    "name": "out", 
                                    "memory": "DDR[2]"
                                }, 
                                {
                                    "name": "encoded_size", 
                                    "memory": "DDR[2]"
                                }, 
                                {
                                    "name": "status_flag", 
                                    "memory": "DDR[2]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR2", 
                            "arguments": [
                                {
                                    "name": "out", 
                                    "memory": "DDR[3]"
                                }, 
                                {
                                    "name": "encoded_size", 
                                    "memory": "DDR[3]"
                                }, 
                                {
                                    "name": "status_flag", 
                                    "memory": "DDR[3]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR2", 
                            "arguments": [
                                {
                                    "name": "out", 
